<LinkTo @route="discovery.latest">
  {{theme-i18n "latest"}}
</LinkTo>

{{#if this.category}}
  <LinkTo @route="discovery.category" @model={{this.categoryModel}}>
    >
    {{this.category.name}}
  </LinkTo>
{{/if}}

{{#if this.topic}}
  <LinkTo @route="topic" @model={{this.topic}}>
    >
    {{this.topic.title}}
  </LinkTo>
{{/if}}