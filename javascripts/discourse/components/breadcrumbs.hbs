<LinkTo @route="discovery.latest" >
  {{theme-i18n "latest"}}
</LinkTo>

{{#if this.category}}
  <LinkTo @route="discovery.category" @model={{this.categoryModel}}>
    > {{this.category.name}}
  </LinkTo>
{{/if}}

{{#if @topic}}
  <LinkTo @route="topic" @model={{@topic}}>
    > {{@topic.title}}
  </LinkTo>
{{/if}}
